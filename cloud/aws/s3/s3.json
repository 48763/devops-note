{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "DelegateS3Access",
            "Effect": "Allow",
            "Principal": {
                "AWS": [
                    "arn:aws:iam::<ACCOUNT_ID>:user/devops",
                    "arn:aws:iam::<ACCOUNT_ID>:user/devops"
                ]
            },
            "Action": "s3:*",
            "Resource": [
                "arn:aws:s3:::<S3_NAME>",
                "arn:aws:s3:::<S3_NAME>/*"
            ]
        }
    ]
}